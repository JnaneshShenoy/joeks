{
   "name": "joeks",
   "version": "0.0.1",
   "manifest_version": 3,
   "action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
   },
   "icons": {
     "128": "icon.png"
   },
   "permissions": ["activeTab", "storage"],
   "background": {
     "service_worker": "background.js",
     "type": "module"
   },
   "content_security_policy": {
     "script-src": "self",
     "object-src": "self"
   }
 }
 